{% trans_default_domain 'app' %}
<table id="resolved-data-requests-table">
    <thead>
        <tr>
            <th>{{ 'form.entity.dataRequest.label.id'|trans }}</th>
            <th>{{ 'form.entity.dataRequest.label.user'|trans }}</th>
            <th>{{ 'form.entity.dataRequest.label.type'|trans }}</th>
            <th>{{ 'form.entity.dataRequest.label.requestedAt'|trans }}</th>
            <th>{{ 'form.entity.dataRequest.label.resolvedAt'|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for dr in resolvedDataRequests %}
            <tr>
                <td>{{ dr.id }}</td>
                <td>{{ dr.userIdentifier }}</td>
                <td>{{ dr.type }}</td>
                <td>{{ dr.requestedAt|date('d.m.Y. H:i') }}</td>
                <td>{{ dr.resolvedAt|date('d.m.Y. H:i') }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>