{% if evaluationEvaluator.type == constant('TYPE_SIMPLE', evaluationEvaluator) %}
    {{ form_start(form, {'attr': {'id': form.vars.id}}) }}
        {{ form_label(form.evaluationQuestion) }}
        {{ form_widget(form.evaluationQuestion) }}

        {{ form_label(form.expectedValue) }}
        {{ form_widget(form.expectedValue) }}

        {{ form_label(form.resultText) }}
        {{ form_widget(form.resultText) }}
    {{ form_end(form) }}
{% endif %}

{% if evaluationEvaluator.type == constant('TYPE_SUM_AGGREGATE', evaluationEvaluator) %}
    {{ form_start(form, {'attr': {'id': form.vars.id}}) }}
    {{ form_label(form.evaluationQuestions) }}
    {{ form_widget(form.evaluationQuestions) }}

    {{ form_label(form.expectedValueRangeStart) }}
    {{ form_widget(form.expectedValueRangeStart) }}

    {{ form_label(form.expectedValueRangeEnd) }}
    {{ form_widget(form.expectedValueRangeEnd) }}

    {{ form_label(form.resultText) }}
    {{ form_widget(form.resultText) }}
    {{ form_end(form) }}
{% endif %}