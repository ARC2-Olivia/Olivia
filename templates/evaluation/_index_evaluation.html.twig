{% trans_default_domain 'app' %}

<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
    <div class="card h-100 p-0" style="min-height: 320px">
        <div class="card-text flex-grow-1 ps-2 pe-2 d-flex justify-content-center align-items-center">
            <div class="text-black">{{ evaluation.name }}</div>
        </div>
        <div class="card-actions ps-2 pe-2 pb-2">
            <a href="{{ path('evaluation_overview', {'evaluation': evaluation.id}) }}" class="btn btn-theme-white bg-blue">{{ 'button.details'|trans }}</a>
        </div>
    </div>
</div>