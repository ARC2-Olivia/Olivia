{% extends 'base.html.twig' %}
{% trans_default_domain 'app' %}

{% block title %}{{ 'page.evaluation.new'|trans }}{% endblock %}

{% block body %}
    <div class="container py-5">
        <div class="row mb-5">
            <h1 class="text-center">{{ 'practicalSubmodule.title.new'|trans }}</h1>
        </div>
        {% include "_partials/toasts.html.twig" with {'classAddition': 'row'} %}
        <div class="row">
            <div class="card">
                <div class="card-text">
                    {% include 'evaluation/_form.html.twig' %}
                </div>
                <div class="card-actions justify-content-end">
                    <button type="submit" class="btn btn-theme-white bg-blue" form="{{ form.vars.id }}">{{ 'common.save'|trans }}</button>
                </div>
            </div>
        </div>
    </div>
{% endblock %}